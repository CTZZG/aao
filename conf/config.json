{
	"log": {
		"access": "/var/log/v2ray/access.log",
		"error": "/var/log/v2ray/error.log",
		"loglevel": "debug"
	},
	"inbound": {
		"listen": "127.0.0.1",
		"port": 30003,
		"protocol": "vmess",
		"settings": {
			"clients": [{
					"id": "CLIENT_ID",
					"alterId": 64,
					"security": "aes-128-gcm"
				}
			]
		},
		"streamSettings": {
			"network": "ws",
			"wsSettings": {
				"path": "/ws"
			}
		}
	},
	"outbound": {
		"protocol": "freedom",
		"settings": {}
	}
}
